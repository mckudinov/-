#include <iostream>
#include <cmath>

using namespace std;

int main() {
    double x1, y1, x2, y2, x3, y3, x4, y4;
    cin >> x1 >> y1 >> x2 >> y2 >> x3 >> y3 >> x4 >> y4;

    double area = 0.5 * abs(x1*(y2-y4) + x2*(y4-y3) + x3*(y3-y1) + x4*(y1-y2));

    if (area == 0) {
        cout << "Точки не образуют выпуклый многоугольник." << endl;
    } else {
        cout << "Точки не образуют выпуклый многоугольник." << endl;
    }

    return 0;
}
